<script>const buttons = document.querySelectorAll('.myButton');
const pageType = document.body.dataset.page || (() => { throw new Error('No data-page attribute found on <body>'); })();
const isIOS = /iP(hone|ad|od)/.test(navigator.userAgent);
const buttonIndex = new Map([...buttons].map(b => [b, { text: b.textContent.trim().toLowerCase(), parent: b.parentElement.tagName === 'A' ? b.parentElement : b }]));
const handleClick = (link, element, type) => {
    const text = element.textContent.trim();
    const schemes = {
        sh: { check: ['whatshub.top/config/', 'whatshub.top/module/'], ios: l => l.includes('whatshub.top/config/') ? `shadowrocket://config/add/${l}` : `shadowrocket://install?module=${l}` },
        st: { check: ['whatshub.top/config/', 'whatshub.top/stoverride/'], ios: l => l.includes('whatshub.top/config/') ? `https://link.stash.ws/install-config/${l.replace(/^https?:\/\//, '')}` : `https://link.stash.ws/install-override/${l.replace(/^https?:\/\//, '')}` },
        qu: { check: ['whatshub.top/rewrite/'], ios: l => l.split('.').pop().toLowerCase() === 'json' ? `quantumult-x:///ui?module=gallery&type=task&action=add&content=["${l}"]` : `quantumult-x:///add-resource?remote-resource={"rewrite_remote":["${l}"]}` },
        lo: { check: ['whatshub.top/config/', 'whatshub.top/plugin/'], ios: l => l.includes('whatshub.top/plugin/') ? `loon://import?plugin=${encodeURIComponent(l)}` : l }
    };
    const scheme = schemes[type];
    if (scheme) {
        const needsCopy = scheme.check.some(c => link.includes(c));
        isIOS ? window.location.href = needsCopy ? scheme.ios(link) : link : needsCopy ? copyToClipboard(link, element, text) : window.location.href = link;
    } else if (type === 'cp') copyToClipboard(link, element, text);
    else if (type === 'tr') window.location.href = link;
    else if (type !== 'in') throw new Error(`Invalid page type: ${type}`);
};
const copyToClipboard = async (link, element, text) => {
    const copy = async () => navigator.clipboard?.writeText(link) || (document.execCommand("copy", false, (t => (t = document.createElement("textarea"), t.value = link, document.body.appendChild(t), t.select(), t))()));
    try {
        await copy();
        element.textContent = "✅链接已复制";
        setTimeout(() => element.textContent = text, 1000);
    } catch (err) {
        console.error('复制失败:', err);
        const temp = document.createElement("textarea");
        temp.value = link;
        document.body.appendChild(temp);
        temp.select();
        document.execCommand("copy");
        document.body.removeChild(temp);
        element.textContent = "✅链接已复制";
        setTimeout(() => element.textContent = text, 1000);
    }
};
const shuffleArray = arr => { for (let i = arr.length - 1; i > 0; i--) { const j = Math.floor(Math.random() * (i + 1)); [arr[i], arr[j]] = [arr[j], arr[i]]; } };
const assignRandomClasses = buttons => {
    const classes = ['color-1', 'color-2', 'color-3', 'color-4'];
    shuffleArray(classes);
    buttons.forEach((b, i) => b.classList.add(classes[i % 4]));
};
const debounce = (fn, wait) => {
    let t;
    return (...args) => (clearTimeout(t), t = setTimeout(() => fn(...args), wait));
};
const initSearch = buttons => {
    const searchBar = document.getElementById("searchBar");
    if (!searchBar) return;
    searchBar.addEventListener("input", debounce(() => {
        const keyword = searchBar.value.trim().toLowerCase();
        requestAnimationFrame(() => buttonIndex.forEach(({ text, parent }) => parent.style.display = text.includes(keyword) ? 'block' : 'none'));
    }, 300));
};
const bindEvents = () => document.addEventListener('click', e => {
    const target = e.target.closest('.myButton[data-href], .container a .myButton');
    if (target) {
        e.preventDefault();
        handleClick(target.dataset.href || target.closest('a').getAttribute('href'), target, pageType);
    }
});
(() => {
    assignRandomClasses(buttons);
    bindEvents();
    initSearch(buttons);
})();</script>
